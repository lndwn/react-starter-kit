{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "jsRules": true,
  "rules": {
    "ordered-imports": false,
    "import-name": false,
    "function-name": false,
    "variable-name": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": [],
  "include": ["**/*.{js,ts,tsx}"],
  "exclude": ["node_modules", ".git"]
}
