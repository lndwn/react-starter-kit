{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "noImplicitAny": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true
  },
  "types": [],
  "include": ["src/**/*.ts", "src/**/*.tsx"]
}
